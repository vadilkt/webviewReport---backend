<!DOCTYPE jasperReport [
        <!ELEMENT jasperReport (parameter|field|title|detail)*>
        <!ELEMENT parameter (#PCDATA)>
        <!ATTLIST parameter
                class CDATA #REQUIRED
                name CDATA #REQUIRED>
        <!ELEMENT field (#PCDATA)>
        <!ATTLIST field
                name CDATA #REQUIRED
                class CDATA #REQUIRED>
        <!ELEMENT title (band)*>
        <!ELEMENT band (textField|subreport)*>
        <!ATTLIST band
                height CDATA #REQUIRED>
        <!ELEMENT textField (reportElement|textFieldExpression)*>
        <!ELEMENT reportElement (#PCDATA)>
        <!ATTLIST reportElement
                height CDATA #REQUIRED
                width CDATA #REQUIRED
                x CDATA #REQUIRED
                y CDATA #REQUIRED>
        <!ELEMENT textFieldExpression (#PCDATA)>
        <!ELEMENT detail (band)*>
        <!ELEMENT subreport (reportElement|subreportExpression|dataSourceExpression)*>
        <!ELEMENT subreportExpression (#PCDATA)>
        <!ELEMENT dataSourceExpression (#PCDATA)>
        ]>
<jasperReport>
<parameter name="provenance" class="java.lang.String"/>
<parameter name="destination" class="java.lang.String"/>
<parameter name="emissionDate" class="java.util.Date"/>
<parameter name="courrierCount" class="java.lang.Integer"/>
<field name="courriersDataSource" class="net.sf.jasperreports.engine.data.JRBeanCollectionDataSource"/>

<title>
<band height="50">
    <textField>
        <reportElement x="0" y="0" width="200" height="20"/>
        <textFieldExpression><![CDATA[$P{provenance}]]></textFieldExpression>
    </textField>
    <textField>
        <reportElement x="0" y="20" width="200" height="20"/>
        <textFieldExpression><![CDATA[$P{destination}]]></textFieldExpression>
    </textField>
</band>
</title>

<detail>
<band height="200">
    <subreport>
        <reportElement x="0" y="0" width="500" height="200"/>
        <subreportExpression><![CDATA["path/to/courierReport.jasper"]]></subreportExpression>
        <dataSourceExpression><![CDATA[$P{courriersDataSource}]]></dataSourceExpression>
    </subreport>
</band>
</detail>
</jasperReport>
